{
  "settings": {
    "migrated": 3,
    "iconPacksPath": ".obsidian/icons",
    "fontSize": 16,
    "emojiStyle": "none",
    "iconColor": null,
    "recentlyUsedIcons": [],
    "recentlyUsedIconsSize": 15,
    "rules": [
      {
        "rule": "^the-vault\\/🔴 Academics\\/📚 Educational Resources\\/🌐 LeetCode$",
        "icon": "SiLeetcode",
        "for": "folders",
        "order": 0,
        "useFilePath": true
      },
      {
        "rule": "^the-vault\\/🔴 Academics\\/📚 Educational Resources\\/🗒️ C\\+\\+$",
        "icon": "TiBrandCpp",
        "for": "folders",
        "order": 1,
        "useFilePath": true
      },
      {
        "rule": "^the-vault\\/anki$",
        "icon": "EiAnki",
        "for": "folders",
        "order": 2,
        "useFilePath": true
      },
      {
        "rule": ".*",
        "icon": "RiFolderFill",
        "for": "folders",
        "order": 3,
        "useFilePath": true
      },
      {
        "rule": "^.*\\.md$",
        "icon": "RiMarkdownLine",
        "for": "files",
        "order": 4,
        "useFilePath": true
      }
    ],
    "extraMargin": {
      "top": 0,
      "right": 4,
      "bottom": 0,
      "left": 0
    },
    "iconInTabsEnabled": true,
    "iconInTitleEnabled": false,
    "iconsBackgroundCheckEnabled": false
  }
}